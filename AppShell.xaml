<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiAppToDo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiAppToDo.View"
    Title="MauiAppToDo">

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate local:LoginView}"
        Route="Login" />

    <ShellContent
        Title="Workspaces"
        ContentTemplate="{DataTemplate local:WorkspaceView}"
        Route="Workspaces" />

</Shell>
